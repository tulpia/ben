<section class="wrapper">
    <article class="header__leftPart">
        <a href="/">
            <img src="{{header.logo["url"]}}" alt="{{header.logo["alt"]}}">
        </a>
    </article>
    <article class="header__rightPart">
        {% if header.links|length > 0 %}
        <ul class="rightPart__links-container">
            {% for link in header.links %}
                <li>
                    <a href="{{link.link["url"]}}" class="btn btn--wide-text" target="_blank">{{link.link["title"]}}</a>
                </li>
                {% if loop.last == false %}
                <li>
                    <span class="text-alt">-</span>
                </li>
                {% endif %}
            {% endfor %}
        </ul>
        {% endif %}
        <a class="btn-mail" data-router-disabled></a>
        <div class="rightPart__btns">
            <a class="btn-menu menu-btn-hover" data-router-disabled>
                <div class="lines-container">
                    <div></div>
                    <div class='wave'></div>
                    <div></div>
                </div>
            </a>
            <a class="btn-close menu-btn-hover" data-router-disabled>
                <div class="first-line"></div>
                <div class="second-line"></div>
            </a>
        </div>
    </article>
</section>