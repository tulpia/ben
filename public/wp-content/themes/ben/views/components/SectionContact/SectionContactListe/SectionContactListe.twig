<section class="block-contact__liste">
    <p class="text liste__title menu-animate">{{title}}</p>
    <ul class="liste__list">
        {% for item in contacts %}
            <li class="menu-animate">
                <div class="icon">
                    <img src="{{item["icone"]["url"]}}" alt="{{item["icone"]["alt"]}}">
                </div>
                <a class="text" href="{{item["link"]["url"]}}">{{item["link"]["title"]}}</a>
            </li>
        {% endfor %}
    </ul>
</section>